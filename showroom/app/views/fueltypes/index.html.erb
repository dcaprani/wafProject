<h1>Listing fueltypes</h1>

<table>
  <tr>
    <th>Fuel type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fueltypes.each do |fueltype| %>
  <tr>
    <td><%= fueltype.fuel_type %></td>
    <td><%= link_to 'Show', fueltype %></td>
    <td><%= link_to 'Edit', edit_fueltype_path(fueltype) %></td>
    <td><%= link_to 'Destroy', fueltype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Fueltype', new_fueltype_path %>
